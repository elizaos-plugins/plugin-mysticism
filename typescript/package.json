{
  "name": "@elizaos/plugin-mysticism",
  "description": "Mystical divination engines for ElizaOS â€” Tarot, I Ching, and Astrology readings",
  "version": "2.0.0-alpha.3",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageType": "plugin",
  "platform": "node",
  "license": "MIT",
  "author": "ElizaOS Contributors",
  "keywords": [
    "plugin",
    "elizaos",
    "mysticism",
    "tarot",
    "iching",
    "astrology",
    "divination"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "peerDependencies": {
    "@elizaos/core": "2.0.0-alpha.3",
    "@elizaos/plugin-form": "2.0.0-alpha.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "scripts": {
    "build": "bun run build.ts",
    "dev": "bun --hot build.ts",
    "test": "vitest run --config vitest.config.ts --passWithNoTests",
    "typecheck": "tsc --noEmit --noCheck",
    "lint": "bunx @biomejs/biome check --write --unsafe .",
    "lint:check": "bunx @biomejs/biome check .",
    "clean": "rm -rf dist .turbo",
    "format": "bunx @biomejs/biome format --write ."
  },
  "publishConfig": {
    "access": "public"
  },
  "agentConfig": {
    "pluginParameters": {
      "MYSTICISM_PRICE_TAROT": {
        "type": "number",
        "description": "Price in SOL for this reading type",
        "required": false,
        "sensitive": false
      },
      "MYSTICISM_PRICE_ICHING": {
        "type": "number",
        "description": "Price in SOL for this reading type",
        "required": false,
        "sensitive": false
      },
      "MYSTICISM_PRICE_ASTROLOGY": {
        "type": "number",
        "description": "Price in SOL for this reading type",
        "required": false,
        "sensitive": false
      }
    }
  }
}
